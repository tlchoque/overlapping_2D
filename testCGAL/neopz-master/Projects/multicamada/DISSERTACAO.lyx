\begin_inset LatexCommand \ref{eq:56}

\end_inset 


\begin_inset LatexCommand \ref{eq:57}

\end_inset 


\begin_inset Formula \begin{equation}
\left\{ u_{n}\right\} _{6\times 1}=\left[\begin{array}{cc}
 [r]_{3\times 3} & 0\\
 0 & [r]_{3\times 3}\end{array}
\right]\left\{ u\right\} _{6\times 1}\label{eq:56}\end{equation}

\end_inset 


\begin_inset Formula \begin{equation}
\left\{ u\right\} _{6\times 1}=\left[\begin{array}{cc}
 [r]_{3\times 3}^{T} & 0\\
 0 & [r]_{3\times 3}^{T}\end{array}
\right]\left\{ u_{n}\right\} _{6\times 1}\label{eq:57}\end{equation}

\end_inset 


\layout Standard

O movimento de translação do ponto 
\begin_inset Formula $P$
\end_inset 

 no sistema 
\begin_inset Formula $SRE$
\end_inset 

 pode ser escrita a partir da translação 
\begin_inset Formula $OO'$
\end_inset 

 e da rotação absoluta do elemento finito 
\begin_inset Formula \begin{equation}
\begin{array}{c}
 u_{e}(e_{1},e_{2},e_{3})=u_{n}(n_{1},n_{2})+(f+e_{3})\theta _{2}\\
 v_{e}(e_{1},e_{2},e_{3})=v_{n}(n_{1},n_{2})-(f+e_{3})\theta _{1}\\
 w_{e}(e_{1},e_{2},e_{3})=w_{n}(n_{1},n_{2})\end{array}
\label{eq:58}\end{equation}

\end_inset 


